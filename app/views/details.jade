extends layout

block content
  h1= title
  
  p #{job.name}
  p #{job.description}
  p #{job.sourceUrl}
  p #{job.targetUrl}
  div(id="images")
    img(class="preview-image" src="data:image/png;base64,#{job.sourceImageId.data}")
    img(class="preview-image" src="data:image/png;base64,#{job.targetImageId.data}")